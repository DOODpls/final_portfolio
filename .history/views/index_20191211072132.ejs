<!DOCTYPE html>
<html lang="en">
<%- include("partials/head") %>
<body>
  <%- include("partials/header") %>
    <div class="banner-container">
      <div class="inner-container">
        <div class="inner-container-text" id="inner-container-text-id"> Floyd Santos</div>
        <div id="webdev" class="webdev" onscroll="scroll()">Web Developer</div>
        <q class="qoute">Work hard, Work fast, then Work smart.</q>
      </div>
    </div>
    <div class="skills-outer-cont" id="skills-outer-cont">
      <div class="skills-inner-cont">
        <div class="top-banner" id="top-banner"> SKILLSET</div>
        <div class="skills-flex" id="skills-flex">
          
          <% pginfo.skills.forEach(function(sklz){ %>
            <div class="skills-logo-desc" id="skills-logo-desc">
              <div class="skill-logo"><i class="<%= sklz.class %>"></i></div>
              <div class="skill-description"><p><%= sklz.desc %></p></div>
            </div>
            <% }) %>
            <% pginfo.extskills.forEach(function(sklzext){ %>
              <div class="skills-logo-desc" id="skills-logo-desc">
                <div class="skill-logo"> <img src="<%= sklzext.imgscr %>" alt="Photoshop" class="external-logo"> </div>
                <div class="skill-description"><p><%= sklzext.desc %></p></div>
              </div>
            <% }) %>
          
        </div>
      </div>
    </div>
    <div class="project-outer-cont">
      <div class="project-inner-cont">
        <div class="top-banner-projects" id="top-banner-projects">Projects</div>
        <span class="anchor" id="project-section"></span>
          <div class="projects-flex">
            <% pginfo.projects.forEach(function(iter){ %>
            <div class="project-box">
              <div class="project-preview"><iframe src="<%= iter.url %>" width="100%" height="100%"></iframe></div>
              <div class="project-name">
                <h2><%= iter.title %></h2>
              </div>
            </div>
            <% }) %>
          </div>
        </div>
      </div>
      <div class="contact-outer-cont">
        <div class="contact-inner-cont">
           <div class="contact-banner">CONTACT ME</div>
           <span class="anchor2" id="contact-section"></span>
            <form action="/sent" class="contact-container">
              <div class="contact-name">
                <p>Name: </p>
                <input type="text" name="contactname" id="contactname" class="contactname">
              </div>
              <div class="contact-email">
                <p>Email: </p>
                <input type="email" name="contactemail" id="contactemail" class="contactemail">
              </div>
              <div class="contact-message">
                <p>Message: </p>
                <textarea name="contactmessage" id="contactmessage" class="contactmessage" cols="30" rows="5"></textarea>
              </div>
              <button type="submit" class="contact-submit">Submit</button>
            </form>
        </div>
      </div>
    <!-- <div class="intro-outer-cont">
      <div class="intro-inner-cont">
        <div class="portrait-holder"></div>
        <div class="about-content-holder">
          <div class="intro">
            About
          </div>
          <div class="intro-par">
            An IT Graduate from the Philippines
          </div>
        </div>
      </div>
    </div>
  <div id="box"></div> -->
<footer class="footer">
  <div class="footer-inner-cont">
    <div class="footer-name">Floyd Santos</div>
    <ul class="footer-social">
      <li class="footer-social"> <a href="https://bit.ly/2Ech3Jz"><i class="fab fa-twitter"></i></a></li>
      <li class="footer-social"> <a href="https://bit.ly/2REPtNd"><i class="fab fa-facebook-f"></i></a></li>
      <li class="footer-social"> <a href="https://bit.ly/34fb2qt"><i class="fab fa-instagram"></i></a></li>
      <li class="footer-social"> <a href="https://bit.ly/2RDMLHO"><i class="fab fa-linkedin-in"></i></a></li>
      <li class="footer-social"> <a href="https://bit.ly/2PaSTpg"><i class="fab fa-github"></i></a></li>
      <li class="footer-social"> <a href="https://bit.ly/35bb0B4"><i class="fab fa-stack-overflow"></i></a></li>
    </ul>
    <div class="copyright">Copyright 2019</div>
  </div>
</footer>
</body>
<script src="js/animations.js"></script> 
